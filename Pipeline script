pipeline {
    agent any

    stages {
        stage('Checkout') {
            steps {
                // Check out the code from the Git repository
                git url: 'https://github.com/yourusername/your-repo.git'
            }
        }
        stage('Build') {
            steps {
                // Replace these commands with your actual build commands
                sh 'npm install'  // Example for Node.js application
                sh 'npm build'
            }
        }
    }
}
